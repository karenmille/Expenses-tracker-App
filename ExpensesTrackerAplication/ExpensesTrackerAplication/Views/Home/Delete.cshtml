@model ExpensesTrackerAplication.Models.Expens
@using (Html.BeginForm("delete", "home", FormMethod.Post, new { id = "Delete" }))
{
    @Html.HiddenFor(a => a.Id)
    <div class="form-group">
        <label>Project ID</label>
        <p>@Model.ProyectID</p>
    </div>

    <div class="form-group">
        <label> Name</label>
        <p>@Model.Name</p>
    </div>

    <div class="form-group">
        <label>Expenses Date</label>
        <p>@Model.ExpenseDate</p>
    </div>

    <div class="form-group">
        <label>Amount</label>
        <p>@Model.Amount</p>
    </div>

    <div class="form-group">
        <label>Description</label>
        <p>@Model.Description</p>
    </div>

    <div>
        <input type="submit" value="Delete" />
        
    </div>
}

